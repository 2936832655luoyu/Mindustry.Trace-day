{
  "type": "liquidRouter",  // 方块类型：指定为液体路由器（原生支持的液体路由类型）
  "size": 1,               // 方块尺寸：1表示1x1大小的方块（纯JSON支持1/2/3等整数）
  "health": 220,           // 生命值：方块的耐久度，受到攻击会减少，为0时被破坏
 "research": "压力管道",  // 解锁该单位所需的研究项目
  "requirements": [        // 建造所需材料：放置方块时消耗的资源
    {"item": "遗湿矿", "amount": 2},  // 石棉x2
    {"item": "copper", "amount": 2}       // 铁x2
  ],
  "category": "liquid",    // 分类：归类到"liquid"（液体）类别，在建造菜单的液体栏中显示
  "liquidCapacity": 20     // 液体容量：方块可存储的最大液体量（单位：单位体积）
 "name": "液体路由器",  // 建筑名称，表明其核心特性是高压输送液体
"description": "用于高效分配液体的枢纽装置，可同时向多个方向传输液体，适配各类液体管道系统（浮空的）。........垃圾，真是个垃圾[生气]",  // 描述文本
"liquidTransfer": 1,  // 每秒液体传输速率（默认有基础值，可自定义）

}
    